<div class="container">
  <h1>User Log</h1>

  <table class="table mt-4">
    <thead>
    <tr>
      <th>Event</th>
      <th>Timestamp</th>
    </tr>
    </thead>

    <tbody>
    <% @user.attendance_logs.each do |log| %>
      <tr>
        <td><%= log.event.name %></td>
        <td><%= log.created_at %></td>
      </tr>
    <% end %>
    </tbody>
  </table>

  <%= link_to 'Back', attendance_user_index_path, :class => "btn btn-primary btn-large" %>
</div>